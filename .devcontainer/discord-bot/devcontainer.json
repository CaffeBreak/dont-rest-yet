{
  "name": "Discord Bot",
  "dockerComposeFile": [
    "../compose.yaml"
  ],
  "service": "discord-bot",
  "workspaceFolder": "/dry",
  "postAttachCommand": "cd DiscordBot && sudo poetry install --no-root",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-azuretools.vscode-docker",
        "yzhang.markdown-all-in-one",
        "EditorConfig.EditorConfig",
        "tamasfe.even-better-toml"
      ]
    }
  }
}
