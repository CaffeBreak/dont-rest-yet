{
  "name": "Discord Bot",
  "dockerComposeFile": [
    "../compose.yaml"
  ],
  "service": "discord-bot",
  "workspaceFolder": "/dry",
  "postAttachCommand": "cd DiscordBot && poetry install && . /dry/DiscordBot/.venv/bin/activate",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-azuretools.vscode-docker",
        "yzhang.markdown-all-in-one",
        "EditorConfig.EditorConfig",
        "tamasfe.even-better-toml",
        "VisualStudioExptTeam.vscodeintellicode",
        "ms-python.python",
        "ms-python.vscode-pylance"
      ]
    }
  }
}
